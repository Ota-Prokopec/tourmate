{
	"private": true,
	"scripts": {
		"build": "dotenv -e .env.development -- turbo run build",
		"dev": "dotenv -e .env.development -- turbo run dev",
		"api": "dotenv -e .env.development -- turbo run dev --filter=api",
		"web": "dotenv -e .env.development -- turbo run dev --filter=web",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"vitest-web": "dotenv -e .env.development -- turbo run vitest --filter=web",
		"build-api": "turbo run build --filter=api"
	},
	"devDependencies": {
		"dotenv-cli": "^7.2.1",
		"eslint": "^7.32.0",
		"eslint-config-custom": "*",
		"prettier": "^2.5.1",
		"turbo": "1.10.4"
	},
	"name": "sveltekit-trpc",
	"packageManager": "yarn@1.22.19",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"dependencies": {
		"deep-equal": "^2.2.2"
	}
}
